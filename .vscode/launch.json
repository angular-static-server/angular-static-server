  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start angular-static-server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["serve"],
      "cwd": "${workspaceFolder}/test/ngssc-app"
    },
    {
      "name": "Compress test app",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["compress"],
      "cwd": "${workspaceFolder}/test/ngssc-app"
    },
  ]
}